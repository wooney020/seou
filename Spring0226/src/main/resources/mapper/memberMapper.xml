<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.swdo.test.dao.MemberMapper">

<insert id="memberInsert"   parameterType="member">

insert into
    member_tb(
        id
        ,pw
        ,name
        ,age
        ,gender
        ,introduce
        
    )values(
        #{id}
        ,#{pw}
        ,#{name}
        ,#{age}
        ,#{gender}
        ,#{introduce}
)
</insert>
</mapper>
